{
  "manifest_version": 2,
  "name": "DigiD QR Fixer",
  "version": "1.3",

  "description": "Removes DigiD logo from QR so it can be scanned more reliably",

  "icons": {
    "48": "icons/digid.svg",
    "96": "icons/digid.svg"
  },

  "content_scripts": [
    {
      "matches": ["https://digid.nl/inloggen_app_qr?*"],
      "js": ["digid-qr-fixer.js"]
    }
  ]
}
